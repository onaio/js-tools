{
  "name": "@onaio/pagination",
  "version": "0.0.4",
  "types": "dist/types",
  "description": "Bootstrap powered pagination component",
  "main": "dist/index.js",
  "repository": "https://github.com/onaio/js-tools",
  "author": "Ona Engineering",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/onaio/js-tools/issues"
  },
  "scripts": {
    "jest": "jest --coverage --verbose --color",
    "tsc": "tsc",
    "build": "yarn run transpile",
    "transpile": "babel src -d dist --root-mode upward --extensions '.ts,.tsx'  --ignore '**/*.test.ts,**/*.test.tsx,**/tests,**/__tests__'",
    "transpile:windows": "babel src -d dist --root-mode upward --extensions .ts,.tsx  --ignore '**/*.test.ts,**/*.test.tsx,**/tests,**/__tests__'"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "setupFiles": [
      "../../setupTests"
    ]
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "reactstrap": "^8.0.1"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.3.1"
  }
}
