{
  "name": "@onaio/element-map",
  "version": "0.0.5",
  "description": "Simple component takes a list of elements and returns them wrapped in the provided HTML tag",
  "keywords": [
    "render lists"
  ],
  "author": "Ona Engineering",
  "homepage": "https://github.com/onaio/js-tools",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/types",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onaio/js-tools.git"
  },
  "scripts": {
    "jest": "jest --coverage --verbose --color",
    "tsc": "tsc",
    "build": "yarn tsc && yarn transpile:windows",
    "transpile": "babel src -d dist --root-mode upward --extensions '.ts,.tsx'  --ignore '**/*.test.ts,**/*.test.tsx,**/tests,**/__tests__'",
    "transpile:windows": "babel src -d dist --root-mode upward --extensions .ts,.tsx  --ignore '**/*.test.ts,**/*.test.tsx,**/tests,**/__tests__'"
  },
  "jest": {
    "setupFiles": [
      "../../setupTests"
    ]
  },
  "bugs": {
    "url": "https://github.com/onaio/js-tools/issues"
  }
}
